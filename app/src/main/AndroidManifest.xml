<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.htt.personallife">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>


    <application
        android:name=".app.PersonalLifeApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.WelcomeActivity"
            android:theme="@style/ImageTranslucentTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activitys.RegisterActivity"/>

        <activity android:name=".activitys.LoginActivity"/>

        <activity android:name=".activitys.MainActivity"/>

        <activity android:name=".activitys.ChatConversationActivity"/>

        <activity android:name=".views.multiphotopicker.ui.activitys.PhotoPagerActivity"/>

        <activity android:name=".views.multiphotopicker.ui.activitys.PhotoPickerActivity"/>

        <activity android:name=".views.ucrop.UCropActivity"/>
    </application>

</manifest>